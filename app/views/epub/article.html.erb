<% present entry do |entry_presenter| %>
<html>
<head>
  <title><%= entry_presenter.sanitized_title %></title>
  <link rel="stylesheet" type="text/css" href="css.css" />
</head>
<body>
  <h1><%= entry_presenter.sanitized_title %></h1>
  <p class="gray">
    <%= entry_presenter.published_date %> <%= entry_presenter.author %>
    <br />
    <a href="<%= entry.fully_qualified_url %>"><%= feed_title %></a>
  </p>
  <%= raw(content) %>
</body>
</html>
<% end %>